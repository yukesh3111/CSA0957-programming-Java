import java.util.*;
public class fib{
public static void main(String[] args){
Scanner obj=new Scanner(System.in);
int a=0,b=1,c=a+b,n;
System.out.print("enter the limit for fibonacci series:");
n=obj.nextInt();
System.out.print(a+" ");
System.out.print(b+" ");
for(int i=3;i<=n;i++){
System.out.print(c+" ");
a=b;
b=c;
c=a+b;
}
int a1=0,b1=-1,c1=a1+b1;
System.out.print(a1+" ");
System.out.print(b1+" ");
for(int j=3;j<=n;j++){
System.out.print(c1+" ");
a1=b1;
b1=c1;
c1=a1+b1;
}
}
}
